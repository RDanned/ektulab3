<Window x:Class="lab.RouteDetail"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab"
        mc:Ignorable="d"
        Title="RouteDetail" Height="450" Width="800">
    <Grid>
        <Image x:Name="RoutePreview" HorizontalAlignment="Left" Height="204" Margin="221,42,0,0" VerticalAlignment="Top" Width="350"/>
        <Label x:Name="RouteName" Content="Label" HorizontalAlignment="Left" Margin="294,251,0,0" VerticalAlignment="Top" Width="198"/>
        <TextBlock x:Name="RouteDescription" HorizontalAlignment="Left" Margin="231,291,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="81" Width="312"/>
    </Grid>
</Window>
